Function Convert-BLGFileToCSV {
param(
[Parameter(Mandatory=$true)][string]$FullFilePath
)
    $parentDirectory = Split-Path -Parent $FullFilePath 
    $fileName = (Split-Path -Leaf $FullFilePath).Replace(".blg","")
    $exportTo = "{0}\{1}_TempData.csv" -f $parentDirectory, $fileName
    [System.Diagnostics.Stopwatch]$stopWatch = [System.Diagnostics.Stopwatch]::StartNew()

    relog $FullFilePath -f csv -o $exportTo 
    $stopWatch.Stop()

}